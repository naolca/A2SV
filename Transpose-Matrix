class Solution:
    def transpose(self, matrix: List[List[int]]) -> List[List[int]]:
        lst=[]
        i=0
        j=0
        while i <len(matrix[0]):
            new_list=[]
            for single in matrix:
                new_list.append(single[j])
            j+=1
            i+=1
            lst.append(new_list)
        return lst

            
