from collections import defaultdict


n = int(input())
grid = []

for  _ in range(n):
    grid.append(list(map(int , input().split())))

d = defaultdict(list)
for r in range(n):
    for c in range(n):
        if  grid[r][c]:
            d[r + 1].append(c + 1)

for i in range(1 , n + 1):
    print(len(d[i]) , *d[i])
